<section id="random-beer">
    <img src="{{image_url}}" width="100">
    <h2>{{name}}</h2>
    <h5>{{tagline}}</h5>
    <p>{{description}}</p>
    <h4>Food Pairing</h4>
    <ul>
        {{#each food_pairing}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <h4>Brewer Tips</h4>
    <p>{{brewers_tips}}</p>
</section>